pipeline {

  agent any
   stages {
     stage ('build'){
     steps { 
      sh  'echo  BUILD'  
        }   
     }
          } 
   stage("build-Junit"){
    steps  {
       sh 'ant -f build.xml -v'
       sh ' ant -f test.xml -v'
       junit 'report/results.xml'
          }
       }
     } 
   post {
   always {
   archiveArtifacts artifacts: 'file.html', fingerprint: true
     } 
   } 
  }
